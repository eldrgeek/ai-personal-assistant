{
  "name": "ai-personal-assistant",
  "version": "0.1.0",
  "description": "AI-powered personal assistant with sprint management, project tracking, rituals, and MCP integration",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python main.py",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "start:backend": "cd backend && python main.py",
    "start:frontend": "cd frontend && npm run dev",
    "test": "concurrently \"npm run test:frontend\" \"npm run test:backend\"",
    "test:frontend": "cd frontend && npm run test",
    "test:backend": "cd backend && python -m pytest",
    "clean": "rm -rf frontend/node_modules backend/__pycache__ backend/*.db",
    "setup": "npm run install:all && npm run dev"
  },
  "keywords": [
    "ai",
    "personal-assistant",
    "productivity",
    "sprint-management",
    "project-tracking",
    "rituals",
    "mcp",
    "react",
    "typescript",
    "python",
    "fastapi"
  ],
  "author": "Mike Wolf",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/ai-personal-assistant.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ai-personal-assistant/issues"
  },
  "homepage": "https://github.com/yourusername/ai-personal-assistant#readme"
}
